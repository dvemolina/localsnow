{
	"name": "localsnow",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:start": "docker compose up",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"machine-translate": "inlang machine translate --project project.inlang",
		"seed": "tsx --tsconfig tsconfig.seeds.json src/lib/server/db/seeds/index.ts",
		"seed:import-csv": "tsx --tsconfig tsconfig.seeds.json src/lib/server/db/seeds/importFromCSV.ts",
		"seed:all": "tsx --tsconfig tsconfig.seeds.json src/lib/server/db/seeds/seedAll.ts",
		"seed:clear": "tsx --tsconfig tsconfig.seeds.json src/lib/server/db/seeds/clearData.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@inlang/cli": "^3.0.0",
		"@inlang/paraglide-js": "2.0.8",
		"@inlang/plugin-m-function-matcher": "^2.1.0",
		"@inlang/plugin-message-format": "^4.0.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.7",
		"@types/pg": "^8.15.6",
		"bits-ui": "^2.14.0",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.30.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.1",
		"globals": "^15.14.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.41.3",
		"svelte-check": "^4.0.0",
		"sveltekit-superforms": "^2.24.1",
		"tailwind-merge": "^3.3.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.7",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.787.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"arctic": "^3.6.0",
		"bcryptjs": "^3.0.2",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.38.4",
		"googleapis": "^164.1.0",
		"mode-watcher": "^0.5.1",
		"paneforge": "1.0.0-next.5",
		"pg": "^8.16.3",
		"postgres": "^3.4.5",
		"sharp": "^0.34.1",
		"stripe": "^19.2.0",
		"svelte-sonner": "^1.0.2",
		"vaul-svelte": "1.0.0-next.7"
	}
}
